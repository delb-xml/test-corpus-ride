              </xs:enumeration>
            </xs:restriction>
          </xs:simpleType>
          <xs:simpleType>
            <xs:restriction base="xs:token">
              <xs:enumeration value="lines">
                <xs:annotation>
                  <xs:documentation>lines of text</xs:documentation>
